{
  "name": "tscurso",
  "version": "1.0.0",
  "description": "curso de typescript",
  "main": "server/server.ts",
  "scripts": {
    "start": "PORT=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "PORT=5000 NODE_ENV=production node ./dist/server/server.js",
    "builded-test": "PORT=5000 NODE_ENV=test node ./dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "coverage-integration": "nyc --reporter=html npm run integration-test",
    "coverage-unit": "nyc --reporter=html npm run unit-test"
  },
  "keywords": [
    "node",
    "sequelize",
    "typescript"
  ],
  "author": "Jivago",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "pg": "^7.3.0",
    "sequelize": "^4.13.8",
    "sequelize-cli": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.16",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.78",
    "@types/mocha": "^2.2.43",
    "@types/morgan": "^1.7.33",
    "@types/passport": "^0.3.4",
    "@types/passport-jwt": "^2.0.24",
    "@types/sequelize": "^4.0.76",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.2.3",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sequelize": "^4.13.8",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.6",
    "ts-node": "^3.3.0"
  }
}